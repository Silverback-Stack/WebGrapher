{
    "Normalisation": {
        "ServiceName": "Normalisation",
        "MaxTitleLength": 100,
        "MaxSummaryWords": 100,
        "MaxKeywords": 300,
        "MaxKeywordTags": 10,
        "MaxLinksPerPage": 100,

        "AllowableLinkSchemas": [
            "http",
            "https"
        ],

        "Processors": {
            "TitleXPath": ".//title",
            "LinksXPath": ".//a[@href]",
            "DefaultLanguageIso2Code": "en",
            "DefaultLanguageIso3Code": "eng"
        }
    },

    "BlobCache": {
        "CacheType": "BlobStorage", //FileStorage|BlobStorage
        "FileStorage": {
            "ContainerName": "file-cache",
            "AbsoluteExpiryHours": 24
        },
        "BlobStorage": {
            "ContainerName": "blob-cache",
            "AbsoluteExpiryHours": 24,
            "ConnectionString": ""
        }
    }
}